\par {\listtext\pard\plain\s26 \f2\fs20 \hich\af2\dbch\af0\loch\f2 3.\tab}StartCliff.HasForestPath = TRUE
\par {\listtext\pard\plain\s26 \f2\fs20 \hich\af2\dbch\af0\loch\f2 4.\tab}StartCliff.HasPineTree = TRUE
\par {\listtext\pard\plain\s26 \f2\fs20 \hich\af2\dbch\af0\loch\f2 5.\tab}StartCliff.NamePhrase = "At 'Start'"
\par }\pard\plain \s28\qj \li0\ri0\sa60\keep\widctlpar\tx288\aspalpha\aspnum\faauto\adjustright\rin0\lin0\itap0 \fs20\lang1033\langfe1033\cgrid\langnp1033\langfenp1033 {
\par \tab Let\rquote s examine each line individually.
